	<bean id="msgbean" class="com.cg.springcore.beans.HelloBean">
		<property name="message" value="Spring Framework is great"></property>
	</bean>

	<bean id="emp1" class="com.cg.springcore.beans.Employee" >
		<property name="empId" value="1111" />
